@model VocaDb.Model.DataContracts.Albums.ArtistForAlbumContract

<tr id="@("artistRow_" + Model.Id)">
	<td>
		@Html.ActionLink(Model.Artist.Name, "Edit", "Artist", new { id = Model.Artist.Id }, null)<br />
		<span class="extraInfo">@Model.Artist.AdditionalNames</span>
	</td>
	<td>
		<input type="button" class="artistRemove" id="@("artistRemove_" + Model.Id)" value="Remove" />
	</td>
</tr>
