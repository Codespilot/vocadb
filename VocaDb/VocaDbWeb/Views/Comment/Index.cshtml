@model IEnumerable<IGrouping<VocaDb.Model.DataContracts.EntryBaseContract, VocaDb.Model.DataContracts.UnifiedCommentContract>>

@{
    ViewBag.Title = ViewRes.Comment.IndexStrings.RecentComments;
    
}

@foreach (var entry in Model) {
    
    @Helpers.EntryLink(entry.Key)<br />
    
    foreach (var comment in entry) {
        @Helpers.PrintComment(comment, false);
    }
    
}