@model VocaDb.Web.Models.GlobalSearchBoxModel

@using (Html.BeginForm("GlobalSearch", "Home", FormMethod.Post)) {
	<p>@Html.TextBox("globalSearchTerm")</p>
	<p>@Html.DropDownListFor(m => m.ObjectType, new SelectList(Model.AllObjectTypes))</p>	
	<p><input type="submit" value="Search" /></p>
}
