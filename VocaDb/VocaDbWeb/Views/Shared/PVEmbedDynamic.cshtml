@using VocaDb.Model.Domain.PVs
@model VocaDb.Model.DataContracts.PVs.PVContract

@if (Model.Service == PVService.NicoNicoDouga) {
	<iframe width="560" height="315" frameborder="0" scrolling="no" src="@Url.Action("PVEmbedNND", "Song", new { pvId = Model.Id })"></iframe>
} else {
	@Helpers.EmbedPV(Model)
}
