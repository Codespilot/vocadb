@using VocaDb.Model.Domain.PVs
@model VocaDb.Web.Models.Shared.PVEmbedParams
		   
@if (Model.PV.Service == PVService.NicoNicoDouga) {
	<iframe width="560" height="315" frameborder="0" scrolling="no" src="@Url.Action("PVEmbedNicoIFrame", "Song", new { pvId = Model.PV.Id })"></iframe>
} else {
	@Helpers.EmbedPV(Model.PV, enableApi: Model.EnableScriptAccess, id: Model.ElementId)
}
