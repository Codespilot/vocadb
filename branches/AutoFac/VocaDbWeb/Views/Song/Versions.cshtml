@model VocaDb.Web.Models.Song.Versions

@{
	ViewBag.Title = ViewRes.EntryDetailsStrings.Revisions + " - " + Model.Song.Name;
	ViewBag.Parents = new[] { 
		Html.ActionLink(ViewRes.SharedStrings.Songs, "Index"), 
		LinkExtensions.ActionLink(Html, Model.Song.Name, "Details", new { id = Model.Song.Id }) 
	};
}

@ArchivedEntryHelpers.CurrentVersionMessage(Model.Song.Version, Model.Song.Status)

@ArchivedEntryHelpers.ArchivedObjectVersions(Model.ArchivedVersions, id => Url.Action("ViewVersion", new { id = id }))